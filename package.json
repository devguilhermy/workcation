{
    "name": "workcation",
    "version": "0.1.0",
    "private": false,
    "dependencies": {
        "@tailwindcss/forms": "^0.3.4",
        "pure-react-carousel": "^1.27.8",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.3.1",
        "tailwindcss": "^2.2.19"
    },
    "devDependencies": {
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "autoprefixer": "^10.4.0",
        "postcss": "^8.4.1",
        "postcss-cli": "^9.0.2",
        "react-scripts": "^4.0.3",
        "typescript": "^4.1.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build-css": "postcss src/css/tailwind.css -o public/css/tailwind-output.css",
        "watch-css": "postcss src/css/tailwind.css -o public/css/tailwind-output.css --watch"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "plugin:jsx-a11y/recommended"
        ],
        "plugins": [
            "jsx-a11y"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
